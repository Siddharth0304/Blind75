********************************************************************* DYNAMIC PROGRAMMING ****************************************************************************
class Solution {
public:
    vector<int> countBits(int n) {
        vector<int> dp(n+1,0);
        dp[0]=0;
        for(int i=1;i<n+1;i++){
            dp[i]=dp[i>>1]+(i&1);
        }
        
        return dp;
    }
};
************************************************************************************************************************************************************************

************************************************************************ BIT MANIPULATION *****************************************************************************

int count(int x){
    int c=0;
    for(int i=0;i<32;i++){
        c=c+x%2;
        x=x>>1;
    }
    return c;
}
class Solution {
public:
    vector<int> countBits(int n) {
        vector<int> dp(n+1,0);
        dp[0]=0;
        for(int i=1;i<n+1;i++){
            dp[i]=count(i);
        }
        
        return dp;
    }
};
*************************************************************************************************************************************************************************
